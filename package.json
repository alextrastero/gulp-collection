{
  "name": "gulp-collection",
  "description": "Gulp plugin to group vinyl files into a collection and create new vinyl files using pagination and permalinks.",
  "version": "0.1.0",
  "homepage": "https://github.com/doowb/gulp-collection",
  "author": "Brian Woodward (https://github.com)",
  "repository": "doowb/gulp-collection",
  "bugs": {
    "url": "https://github.com/doowb/gulp-collection/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "collection",
    "gulp"
  ],
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "group-array": "^0.3.1",
    "lazy-cache": "^2.0.1",
    "paginationator": "^0.1.4",
    "placeholders": "^0.3.0",
    "through2": "^2.0.1",
    "vinyl": "^1.1.1"
  },
  "files": [
    "index.js",
    "lib/",
    "utils.js"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "group-array",
        "paginationator",
        "permalinks",
        "placeholders"
      ]
    },
    "reflinks": [
      "gulp",
      "verb",
      "verb-readme-generator",
      "gulp-gray-matter",
      "handlebars",
      "vinyl",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-extname": "^0.2.2",
    "gulp-format-md": "^0.1.9",
    "gulp-gray-matter": "^3.0.1",
    "handlebars": "^4.0.5"
  }
}
